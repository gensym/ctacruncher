<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <servlet>
        <servlet-name>datafetcher</servlet-name>
        <servlet-class>api.datafetcher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>structure-fetched-data</servlet-name>
        <servlet-class>api.structure_fetched_data</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>structure-fetched-data</servlet-name>
        <url-pattern>/structure-fetched-data</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>/index.html</welcome-file>
    </welcome-file-list>
</web-app>